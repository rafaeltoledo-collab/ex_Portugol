programa {
	
	funcao inicio() {
		inteiro numero1, numero2
		escreva("Digite um número: ")
		leia(numero1)
		escreva("Outro número: ")
		leia(numero2)
		
		escreva("o resto da divisão de ", numero1, " por ", numero2,  " é: ", numero1 % numero2)
          
	}
}
